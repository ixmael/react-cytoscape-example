<!doctype html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1, user-scalable=no" />
    <meta name="author" content="Ismael Ramos Merlos">
      <meta name="description" content="Interacción con gráficas.">

    <title>Gráfica</title>

    <% for (var css in htmlWebpackPlugin.files.css) { %>
    <link href="<%= htmlWebpackPlugin.files.css[css] %>" rel="stylesheet">
    <% } %>
</head>
<body>
    <nav class="navbar navbar-default">
        <div class="container-fluid">
            <div class="navbar-header">
                <button class="navbar-toggle" type="button" data-toggle="collapse" data-target="#navbar-main">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Gráfica</a>
            </div>

            <div id="graph-nodes">
                <p class="navbar-text">Cargando...</p>
                <ul class="nav navbar-nav">
                    <li><a id="data-source-link" href="data/graph.json">Gráfica (JSON)</a></li>
                </ul>
            </div>
        </div>
    </nav>
    
    <div class="container">
        <div id="graph" class="container">
            <div id="cy-node" class="col-lg-12 col-md-12 col-sm-12 col-xs-12"></div>
        </div>
    </div>

    <% for (var chunk in htmlWebpackPlugin.files.chunks) { %>
    <script src="<%= htmlWebpackPlugin.files.chunks[chunk].entry %>"></script>
    <% } %>
</body>
</html>
